{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase": "error",
        "spaced-comment": "error",
        "quotes": ["error", "single", { "avoidEscape": true }],
        "no-duplicate-imports": "error"
    }
}
